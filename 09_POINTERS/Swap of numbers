#include <stdio.h>

void swap(int *a, int *b) {
    int temp;
    temp = *a;
    *a = *b;
    *b = temp;
}

int main() {
    int x, y;

    // Input values
    printf("Enter two numbers: ");
    scanf("%d %d", &x, &y);

    // Before swap
    printf("Before swap: x = %d, y = %d\n", x, y);

    // Call swap function
    swap(&x, &y);

    // After swap
    printf("After swap: x = %d, y = %d\n", x, y);

    return 0;
}

